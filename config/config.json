{
  "excel": {
    "template_relative": "data/templates/Plantilla Extension Materiales.xlsx",
    "sheet_name": "Hoja1",
    "required_columns": ["Codigo", "Centro", "CentroModelo", "CentroBeneficio", "Secuencia", "Estado", "TipoMaterial"],
    "dtype": {
      "Codigo": "string",
      "Centro": "string",
      "CentroModelo": "string",
      "CentroBeneficio": "Float64",
      "Secuencia": "string",
      "Estado": "string",
      "TipoMaterial": "string"
    }
  },
  "output": {
    "txt_filename": "archivo_cargue.txt",
    "delimiter": "\t"
  },
  "logging": {
    "level": "INFO",
    "rotate_when": "D",
    "rotate_interval": 7,
    "backup_count": 4
  }
}